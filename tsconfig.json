{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "module": "CommonJS",
    "noImplicitAny": true,
    "removeComments": true,
    "sourceMap": true,
    "outDir": "./dist",
    "strict": true,
    "jsx": "react-jsx",
    "esModuleInterop": true
},
"include": [
    "./**/*"
],
"exclude": [
    "node_modules"
]
}
